{
  "userRoute": [
    {
      "name": "home",
      "path": "/home",
      "component": "layout.base$view.home",
      "meta": {
        "title": "首页",
        "i18nKey": "route.home",
        "icon": "mdi:monitor-dashboard",
        "order": 1
      }
    },
    {
      "name": "message",
      "path": "/message",
      "component": "layout.base$view.message",
      "meta": {
        "title": "消息查看",
        "i18nKey": "route.message",
        "hideInMenu": true,
        "icon": "material-symbols:mail-outline",
        "order": 2
      }
    },
    {
      "name": "user-center",
      "path": "/user-center",
      "component": "layout.base$view.user-center",
      "meta": {
        "title": "个人中心",
        "i18nKey": "route.user-center",
        "hideInMenu": true,
        "icon": "ph:user-circle",
        "order": 2
      }
    }
  ],
  "constantRoute": [
    {
      "name": "login",
      "path": "/login/:module(pwd-login|code-login|register|reset-pwd|bind-wechat)?",
      "component": "layout.blank$view.login",
      "props": true,
      "meta": {
        "title": "login",
        "i18nKey": "route.login",
        "constant": true,
        "hideInMenu": true
      }
    },
    {
      "name": "403",
      "path": "/403",
      "component": "layout.blank$view.403",
      "meta": {
        "title": "403",
        "i18nKey": "route.403",
        "constant": true,
        "hideInMenu": true
      }
    },
    {
      "name": "404",
      "path": "/404",
      "component": "layout.blank$view.404",
      "meta": {
        "title": "404",
        "i18nKey": "route.404",
        "constant": true,
        "hideInMenu": true
      }
    },
    {
      "name": "500",
      "path": "/500",
      "component": "layout.blank$view.500",
      "meta": {
        "title": "500",
        "i18nKey": "route.500",
        "constant": true,
        "hideInMenu": true
      }
    },
    {
      "name": "iframe-page",
      "path": "/iframe-page/:url",
      "component": "layout.base$view.iframe-page",
      "props": true,
      "meta": {
        "title": "iframe-page",
        "i18nKey": "route.iframe-page",
        "constant": true,
        "hideInMenu": true,
        "keepAlive": true
      }
    }
  ]
}
